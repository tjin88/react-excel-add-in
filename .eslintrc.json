{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": [
    "office-addins"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:office-addins/react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off"
  }
}
